{
  "name": "momotphoto",
  "version": "1.0.0",
  "description": "Photography Site",
  "author": "",
  "scripts": {
    "serve": "npm run pre-build && vue-cli-service serve",
    "build": "npm run pre-build && vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-album-json": "node api/AlbumFileGenerator.js",
    "create-thumbnails": "node api/ThumbnailCreator.js",
    "pre-build": "npm run build-album-json && npm run create-thumbnails",
    "start": "node api/index.js",
    "test": "test"
  },
  "main": "index.js",
  "dependencies": {
    "@mdi/font": "^4.5.95",
    "apicache": "^1.5.2",
    "body-parser": "^1.19.0",
    "bricks.js": "^1.8.0",
    "connect-history-api-fallback": "^1.6.0",
    "core-js": "^3.4.1",
    "directory-tree": "^2.2.4",
    "dns": "^0.2.2",
    "exif": "^0.6.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "image-size": "^0.8.3",
    "imagesloaded": "^4.1.4",
    "node-fetch": "^2.6.0",
    "promise": "^8.0.3",
    "roboto-fontface": "*",
    "sharp": "^0.23.3",
    "v-photoswipe": "^1.0.3-1",
    "vue": "^2.6.10",
    "vue-cli-plugin-vuetify": "^2.0.2",
    "vue-router": "^3.1.3",
    "vuetify": "^2.1.0"
  },
  "devDependencies": {
    "@vue/cli": "^4.0.5",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "compression": "^1.7.4",
    "eslint": "^6.6.0",
    "eslint-plugin-vue": "^6.0.1",
    "mocha": "^6.2.2",
    "mock-require": "^3.0.3",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-vuetify": "^2.0.2",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "bugs": {
    "url": "https://github.com/edmomot/momotphoto/issues"
  },
  "engines": {
    "node": "12.13.0"
  },
  "homepage": "https://github.com/edmomot/momotphoto#readme",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edmomot/momotphoto.git"
  }
}
